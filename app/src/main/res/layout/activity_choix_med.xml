<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".choixMed">

    <Button
        android:id="@+id/dateMed"
        android:layout_width="144dp"
        android:layout_height="101dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/buttonf"
        android:drawableTop="@drawable/ic_date_range_black_24dp"
        android:text="APPOINTEMENT"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <Button
        android:id="@+id/searchMed"
        android:layout_width="144dp"
        android:layout_height="101dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="39dp"
        android:background="@drawable/buttonf"
        android:drawableTop="@drawable/patients"
        android:text="Patients
"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateMed" />

    <Button
        android:id="@+id/ProfileMed2"
        android:layout_width="144dp"
        android:layout_height="101dp"
        android:layout_marginStart="91dp"
        android:layout_marginLeft="91dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/buttonf"
        android:drawableTop="@drawable/ic_person_outline_black_24dp"
        android:text="PROFILE"
        app:layout_constraintStart_toEndOf="@+id/dateMed"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/Lout"
        android:layout_width="144dp"
        android:layout_height="101dp"
        android:layout_marginStart="91dp"
        android:layout_marginLeft="91dp"
        android:layout_marginTop="39dp"
        android:background="@drawable/buttonf"
        android:drawableTop="@drawable/ic_exit_to_app_black_24dp"
        android:onClick="logout"
        android:text="LOG OUT"
        app:layout_constraintStart_toEndOf="@+id/searchMed"
        app:layout_constraintTop_toBottomOf="@+id/ProfileMed2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="179dp"
        android:layout_height="29dp"
        android:layout_marginStart="108dp"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="160dp"
        android:text="Welcome User"
        android:textColor="#33AEB6"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />
</androidx.constraintlayout.widget.ConstraintLayout>